---
import SideNav from 'components/Dashboard/Common/SideNav';
import DashNav from 'components/Dashboard/Common/DashNav';
import Layout from 'layouts/Layout.astro';
import CalendarSchedular from 'components/Dashboard/Account/CalendarSchedular';

const cookie  = Astro.request.headers.get('cookie');
if (!cookie.includes('session')) {
  return Astro.redirect('/login');
}

---

<Layout title="Cases">
	<DashNav client:load/>
	<SideNav activeMenu="account" />
    <CalendarSchedular client:load/>
</Layout>

<style is:global>
	body {
		font-family: Poppins;
	}
</style>
