---
import Cases from 'components/Dashboard/Cases';
import SideNav from 'components/Dashboard/Common/SideNav';
import DashNav from 'components/Dashboard/Common/DashNav';
import Layout from 'layouts/Layout.astro';

const cookie = Astro.request.headers.get('cookie');
if (!cookie.includes('session')) {
  return Astro.redirect('/login');
}

---

<Layout title="Cases">
	<DashNav />
	<SideNav activeMenu="cases" />
	<Cases client:load />
</Layout>

<style is:global>
	body {
		font-family: Poppins;
	}
</style>

