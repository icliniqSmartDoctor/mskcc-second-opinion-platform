---
import Layout from "layouts/Layout.astro";
import Footer from "components/Common/Footer";
import Nav from "components/Common/Nav";
import MenuNav from "components/Common/MenuNav";
import DoctorInfo from "components/Home/DoctorInfo";
import OtherCommonCancer from "components/OtherTypeCancer/OtherCommonCancer";
---

<Layout title="Other Common Types of Cancer | MSKCC-India">
    <meta 
		name="title" 
		content="Other Common Types of Cancer | MSKCC-India"
		slot="meta-header" 
	/>
	<meta 
		name="description" 
		content="Learn more about the other commonly occurring types of cancer and their characteristics."
		slot="meta-header"
	/>
	<meta 
		name="Keywords" 
		content="Cancer, types of cancer"
		slot="meta-header"
	/>
	<link 
		rel="canonical" 
		href="https://mskcc.icliniq.com/cancer-care/types/other-common-types-of-cancer"
        slot="meta-header"
	/>
    <Nav client:idle/>
    <MenuNav />
    <div class="container-alternate pt-20 pb-20">
        <h2 class="text-md font-semibold text-gray-600 mb-12">
            We are here to care for you.
        </h2>
        <div class="md:flex justify-between">
            <h1 class="text-4xl md:text-6xl font-semibold">Other Types of Cancer</h1>
            <div class="mr-6 font-medium pt-4 text-md md:text-lg md:font-medium">
                <a href="https://mskcc.icliniq.com/" class="text-gray-400 mr-2">Home</a>
                <span class="mr-2 text-gray-400">|</span>
                <a class="text-primary mr-2">Other Types of Cancer</a>
            </div>
        </div>
    </div>
    <div class="gray-bg pt-12 pb-28">
        <div class="container-alternate pt-0 lg:pt-20 md:pt-20 sm:pt-20">
            <div class="grid grid-cols-3 gap-8">
                <OtherCommonCancer/>
            </div>
        </div>
    </div>
    <Footer />
</Layout>
